-- Scripts:

local function GKGI_fake_script() -- kat.Make GUI Draggable 
	local script = Instance.new('LocalScript', kat)

	local UserInputService = game:GetService("UserInputService")
	
	local gui = script.Parent
	
	local dragging
	local dragInput
	local dragStart
	local startPos
	
	local function update(input)
		local delta = input.Position - dragStart
		gui.Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X, startPos.Y.Scale, startPos.Y.Offset + delta.Y)
	end
	
	gui.InputBegan:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
			dragging = true
			dragStart = input.Position
			startPos = gui.Position
	
			input.Changed:Connect(function()
				if input.UserInputState == Enum.UserInputState.End then
					dragging = false
				end
			end)
		end
	end)
	
	gui.InputChanged:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
			dragInput = input
		end
	end)
	
	UserInputService.InputChanged:Connect(function(input)
		if input == dragInput and dragging then
			update(input)
		end
	end)
end
coroutine.wrap(GKGI_fake_script)()
local function NEUCD_fake_script() -- KatGui.KeyBind to Open GUI 
	local script = Instance.new('LocalScript', KatGui)

	local FrameObject = script.Parent.kat --Directs to the Mainframe/MainGUI that needs to be made visible
	local Open = false --Lets the script know if the GUI is visible or not
	
	local UserInputService = game:GetService("UserInputService") --Directs to InputService to pickup what key the user presses
	
	UserInputService.InputBegan:Connect(function(KeyCode) --Connects function
		if KeyCode.KeyCode == Enum.KeyCode.LeftAlt then --(CHANGE "RightShift" TO WHATEVER YOU WANT YOUR KEYBIND TO BE) --Tells the script which key is needed to be clicked 
			if Open then --If the gui is open then close
				FrameObject.Visible = false --Makes the gui invisible
				Open = false --lets the script know the gui is closed
			else --if the gui is not open then
				Open = true --Lets the script know the gui is opened
				FrameObject.Visible = true --makes the gui visible
			end --ends variable
		end --ends variable
	end) --ends script
end
coroutine.wrap(NEUCD_fake_script)()
